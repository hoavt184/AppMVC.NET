@model App.Models.PlanetModel


<h1>@Model.Name</h1>

<div class="card w-58">
    <img class="card-img-top" src="~/images/planets/@(Model.Name).jpg" alt="">
    <div class="card-body">
        <h5>@Model.VnName</h5>
        <div class="card-text">
            @Model.Content
        </div>
        <a class="card-link" asp-action="Index">Các hành tinh</a>
    </div>
</div>